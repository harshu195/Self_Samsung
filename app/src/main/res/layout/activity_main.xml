<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:id="@+id/container"
    android:layout_width="match_parent" android:layout_height="match_parent"
    tools:context="appsandmaps.temple.edu.self.MainActivity" tools:ignore="MergeRootFrame">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/Cyan"
        android:id="@+id/Main_tab"
        android:paddingLeft="10dp"
        >


        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_marginRight="30dp"
            android:background="@null"
            android:src="@mipmap/ic_settings_applications_black_24dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />
        <ImageButton
            android:id="@+id/imageButton1"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_marginRight="30dp"
            android:background="@null"
            android:src="@mipmap/ic_home_white_24dp"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/imageButton3"
            android:layout_marginEnd="95dp" />
        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_marginRight="30dp"
            android:background="@null"
            android:src="@mipmap/ic_local_dining_black_24dp"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/imageButton5" />
        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_marginRight="30dp"
            android:background="@null"
            android:src="@mipmap/ic_data_usage_black_24dp"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/imageButton6"
            android:layout_marginEnd="72dp" />
        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_marginRight="30dp"
            android:background="@null"
            android:src="@mipmap/ic_dashboard_black_24dp"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/imageButton4"
            android:layout_marginEnd="245dp" />
        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_marginRight="30dp"
            android:background="@null"
            android:src="@mipmap/ic_view_list_black_24dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true" />

    </RelativeLayout>

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/LightCyan"
        android:layout_gravity="center"
        android:layout_below="@+id/Main_tab"
        android:layout_alignParentStart="true"
        android:id="@+id/Second_tab"
        android:paddingLeft="10dp">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Jeff"
            android:id="@+id/textView"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true" />

    </RelativeLayout>

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="fill_parent"
        android:layout_below="@+id/Second_tab"
        android:layout_height="fill_parent"
        android:id="@+id/container1">

    </RelativeLayout>

</RelativeLayout>
