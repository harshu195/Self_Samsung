<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="appsandmaps.temple.edu.self.MainActivity$PlaceholderFragment"
    android:background="@color/White">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:id="@+id/imageView"
        android:src="@drawable/jeff_avatar"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />



    <FrameLayout
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:id="@+id/fragment_SSEnergy"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="37dp" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_above="@+id/fragment_SSEnergy"
        android:layout_centerHorizontal="true">
        <FrameLayout
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:id="@+id/fragment_goal"
            android:layout_margin="1dp" />
        <FrameLayout
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:id="@+id/fragment_level"/>
        <FrameLayout
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:id="@+id/fragment_rank"
            android:layout_margin="1dp" />
    </LinearLayout>
</RelativeLayout>
